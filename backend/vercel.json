{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/backend/static/$1"
    },
    {
      "src": "/webhook",
      "dest": "/backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/backend/main.py"
    }
  ]
}