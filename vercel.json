{
  "version": 2,
  "builds": [
    {
      "src": "backend/frontend/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://mellstar-backend.onrender.com/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/backend/frontend/$1"
    }
  ]
}